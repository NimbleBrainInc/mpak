{
  "name": "@nimblebrain/mpak-registry",
  "version": "0.1.0",
  "description": "mpak registry server - bundle, skill, and MCP server registry",
  "main": "dist/index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts | npx pino-pretty",
    "build": "npm run db:generate && tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "typecheck": "npm run db:generate && tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "dotenv -e .env -- prisma migrate dev",
    "db:seed": "dotenv -e .env -- tsx prisma/seed.ts",
    "clean": "rm -rf dist"
  },
  "license": "Apache-2.0",
  "author": "NimbleBrain Inc <engineering@mpak.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/NimbleBrainInc/mpak.git",
    "directory": "apps/registry"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/cloudfront-signer": "^3.957.0",
    "@clerk/backend": "^2.29.0",
    "@fastify/cors": "^11.2.0",
    "@fastify/rate-limit": "^10.0.0",
    "@fastify/multipart": "^9.3.0",
    "@fastify/sensible": "^6.0.4",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@kubernetes/client-node": "^1.4.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "adm-zip": "^0.5.16",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "fastify-plugin": "^5.0.1",
    "jose": "^6.1.3",
    "pg": "^8.13.1",
    "prisma": "^7.2.0",
    "semver": "^7.6.3",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/node": "^22.19.3",
    "@types/pg": "^8.16.0",
    "@types/semver": "^7.5.8",
    "@vitest/coverage-v8": "^4.0.16",
    "dotenv-cli": "^11.0.0",
    "pino-pretty": "^13.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.16"
  }
}
